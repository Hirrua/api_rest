generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String   @id @default(uuid())
  name       String
  email      String   @unique
  password   String
  cpf        String   @db.VarChar(14)
  rg         String   @db.VarChar(14)
  created_at DateTime @default(now())

  @@map("users")
}
